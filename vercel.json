{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/embed.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "functions": {
    "src/api/embed.py": {
      "runtime": "python@3.9"
    },
    "src/index.ts": {
      "runtime": "nodejs@18.x"
    }
  }
}
